{
  "name": "@repo/server",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build && npm run dev:watch",
    "dev:watch": "concurrently \"rollup --config rollup.config.ts --configPlugin rollup-plugin-esbuild --watch\" \"nodemon -w dist/index.js dist/index.js\"",
    "build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-esbuild",
    "start": "npm run build && node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/export.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/client-secrets-manager": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/session": "^10.9.0",
    "@fastify/static": "^7.0.4",
    "@prisma/client": "^6.18.0",
    "@repo/api": "*",
    "@repo/app-config": "*",
    "@repo/hashcat": "*",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "bcrypt": "^5.1.1",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1"
  },
  "devDependencies": {
    "@repo/cluster": "*",
    "@repo/typescript-config": "*",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.3.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "rollup": "^4.20.0",
    "rollup-plugin-esbuild": "^6.1.1"
  }
}
